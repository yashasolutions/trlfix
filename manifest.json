{
  "manifest_version": 3,
  "name": "TRL Microphone issue",
  "version": "1.0",
  "description": "Fix issue with Microphone bug",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": [
    "https://trl.kab.info/*"
  ]
}

